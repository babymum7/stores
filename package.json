{
  "name": "stores",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "10.11.0"
  },
  "now": {
    "dotenv": "variables.env.now"
  },
  "scripts": {
    "start": "node ./server.js",
    "check": "eslint --ignore-pattern public/ --ignore-pattern node_modules/ \"./**/*.js\"",
    "fix": "eslint --ignore-pattern public/ --ignore-pattern node_modules/ --fix \"./**/*.js\"",
    "dev": "concurrently --names \"💻,🌐,📦\" --prefix \"{index}-{name}\" \"npm run dev:be\" \"npm run serve\" \"npm run dev:fe:w\"",
    "serve": "gulp serve",
    "dev:be": "nodemon --ext js --ignore public/ --ignore src/ ./server.js",
    "dev:fe": "export \"NODE_ENV=development\" || SET \"NODE_ENV=development\" && webpack || exit 0",
    "dev:fe:w": "export \"NODE_ENV=development\" || SET \"NODE_ENV=development\" && webpack -w --display-max-modules 0",
    "prod": "webpack --env.mode=production || exit 0",
    "sample": "node ./data/load-sample-data.js",
    "blowitallaway": "node ./data/load-sample-data.js --delete"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "connect-mongodb-session": "^2.0.3",
    "dompurify": "^1.0.8",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "html-to-text": "^4.0.0",
    "jimp": "^0.5.3",
    "jsdom": "^12.1.0",
    "juice": "^5.0.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "mongoose": "^5.2.16",
    "multer": "^1.4.0",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "pug": "^2.0.3",
    "slugs": "^0.1.3",
    "uuid": "^3.3.2",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.24.7",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.3",
    "dotenv": "^6.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-pug": "^4.0.1",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "postcss-loader": "^3.0.0",
    "postcss-uncss": "^0.16.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uncss": "^0.16.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "browserslist": [
    "cover 99.5%",
    "> 0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "commonjs": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": "airbnb",
    "rules": {
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "next"
        }
      ],
      "no-console": 0,
      "comma-dangle": 0,
      "eol-last": 0,
      "global-require": 0,
      "arrow-parens": 0,
      "consistent-return": 0,
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ],
      "func-names": [
        0,
        "as-needed"
      ],
      "space-before-function-paren": 0,
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "user",
            "_id"
          ]
        }
      ]
    },
    "plugins": [
      "html"
    ]
  },
  "eslintIgnore": [
    "/src/js"
  ]
}
